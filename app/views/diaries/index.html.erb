<div class="index_content">
  <div class="atunou_index"><%= link_to "熟練農家の投稿", atunou_path, method: :get %></div>
  <div class="question_index"><%= link_to "質問一覧", questions_path, method: :get %></div>
</div>
  <div class= "search">
    <%= form_with(scope: :search, url: diaries_path, method: :get, local: true) do |f| %>
        <%= f.text_field :vegtable, value: @search_params[:vegtable],placeholder:"🔍野菜を入力" %>
        <%= f.submit "検索" %>
    <% end %>
  </div>

<div id="new_open">
  <div class="new_question">
    <%= link_to "質問してみる",  new_question_path,method: :get %>
  </div>
  <div class="new_diary">
    <%= link_to "投稿する", new_diary_path,method: :get %>
  </div>
</div>




  <tr id="diary_<%= @diaries.ids %>">
    <%= render partial: 'diaries', collection: @diaries, as: "d"  %>
  </tr>

 <%= paginate(@diaries) %>
